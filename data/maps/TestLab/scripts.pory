mapscripts TestLab_MapScripts {
    MAP_SCRIPT_ON_TRANSITION: TestLab_EventScript_BallDefault
}
 
script TestLab_EventScript_BallDefault{
    setvar(VAR_UNUSED_0x40FF, 0)
}
 
script TestLab_EventScript_Birch{
    lockall
    faceplayer
    if(!(flag(FLAG_UNUSED_0x04F) || flag(FLAG_UNUSED_0x04E) || flag(FLAG_UNUSED_0x04D))){
        msgbox("Hello, would you like to see more?", MSGBOX_YESNO)
        if(var(VAR_RESULT) == YES){
            if(var(VAR_UNUSED_0x40FF) != 2){
                addvar(VAR_UNUSED_0x40FF, 1)
            }
            else{
                setvar(VAR_UNUSED_0x40FF, 0)
            }
            msgbox("Choose your Pokemon wisely")
        }
        else{
            msgbox("Choose your Pokemon wisely")
        }
    }
    else{
        msgbox("Take care of your Pokemon")
    }
    waitmessage
    releaseall
    end
}
 
script TestLab_EventScript_PokeballOne{
    lockall
    if(!(flag(FLAG_UNUSED_0x04F) || flag(FLAG_UNUSED_0x04E) || flag(FLAG_UNUSED_0x04D))){
        if(var(VAR_UNUSED_0x40FF) == 0){
            call(TestLab_EventScript_Sprigatito)
        }
        elif(var(VAR_UNUSED_0x40FF) == 1){
            call(TestLab_EventScript_Grookey)
        }
        elif(var(VAR_UNUSED_0x40FF) == 2){
            call(TestLab_EventScript_Chespin)
        }
    }
    else{
        msgbox("Don't Take Another")
    }
    waitmessage
    releaseall
    end
}
 
